		<s:update var="result" dataSource="java/mySqlDB">
			UPDATE test_member SET pass = ? WHERE num = ? AND pass = ?;
			<s:param>${param.pass}</s:param>
			<s:param>${param.num}</s:param>
			<s:param>${param.pass}</s:param>
		</s:update>
	</c:catch>
	<c:if test="${!empty e}">
